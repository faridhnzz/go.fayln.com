{
  "version": 2,
  "regions": ["sin1"],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Referrer-Policy",
          "value": "no-referrer"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://fayln.com"
    },
    {
      "source": "/github/:path*",
      "destination": "https://github.com/faridhnzz/:path*"
    },
    {
      "source": "/instagram",
      "destination": "https://www.instagram.com/faridhnzz"
    },
    {
      "source": "/twitter",
      "destination": "https://twitter.com/faridhnzz"
    },
    {
      "source": "/polywork",
      "destination": "https://www.polywork.com/faridnizam"
    }
  ],
  "rewrites": [{ "source": "/status", "destination": "/status.html" }]
}
